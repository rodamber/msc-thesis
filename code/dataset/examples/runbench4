#!/bin/bash
TIMESTAMP=$(date '+%F-%T')
OUTDIR=out-$TIMESTAMP
EXAMPLES=$1
CONSTS=$2

mkdir $OUTDIR
touch $OUTDIR/command.txt
printf '%s\n' "runbench -t 600 -m 16 -d $OUTDIR -e $EXAMPLES -c $CONSTS" > $OUTDIR/command.txt

runbench -t 600 -m 16 -d $OUTDIR -e $EXAMPLES -c $CONSTS examples-{01..25}.json &
runbench -t 600 -m 16 -d $OUTDIR -e $EXAMPLES -c $CONSTS examples-{26..50}.json &
runbench -t 600 -m 16 -d $OUTDIR -e $EXAMPLES -c $CONSTS examples-{51..75}.json &
runbench -t 600 -m 16 -d $OUTDIR -e $EXAMPLES -c $CONSTS examples-{76..95}.json &
